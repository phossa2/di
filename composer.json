{
    "name": "phossa2/di",
    "license": "MIT",
    "type": "library",
    "description": "A very nice dependency injection library for PHP.",
    "keywords": ["phossa2", "dependency injection", "container"],
    "homepage": "https://github.com/phossa2/di",
    "authors": [
        {
            "name": "Hong Zhang",
            "homepage": "https://github.com/phossa2"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "phossa2/shared": "dev-master",
        "phossa2/config": "dev-master",
        "container-interop/container-interop": "~1.0"
    },
    "autoload": {
        "psr-0": { "Phossa2\\Di\\": "src/" }
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "config": {
        "platform": {
            "php": "5.4.0"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "archive": {
        "exclude": [ "!vendor", "tests", "composer.lock" ]
    }
}
